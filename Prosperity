                updateSectionFrame()
                UpdateSize()

                btn.MouseButton1Click:Connect(function()
                    if not focusing then
                        if toggled == false then
                            game.TweenService:Create(img, TweenInfo.new(0.11, Enum.EasingStyle.Linear,Enum.EasingDirection.In), {
                                ImageTransparency = 0
                            }):Play()
                        else
                            game.TweenService:Create(img, TweenInfo.new(0.11, Enum.EasingStyle.Linear,Enum.EasingDirection.In), {
                                ImageTransparency = 1
                            }):Play()
                        end
                        toggled = not toggled
                        pcall(callback, toggled)
                    end
                end)
            end

            function Elements:NewDropdown(dropname, list, callback)
                local DropFunction = {}
                dropname = dropname or "Dropdown"
                list = list or {}
                callback = callback or function() end   

                local opened = false
                local DropYSize = 33

                local dropFrame = Instance.new("Frame")
                local UIListLayout = Instance.new("UIListLayout")
                local dropOpenbtn = Instance.new("TextButton")
                local dropInfo = Instance.new("TextLabel")
                local coverup = Instance.new("Frame")
                local dropClosed = Instance.new("ImageLabel")
                local dropOpen = Instance.new("ImageLabel")

                dropFrame.Name = "dropFrame"
                dropFrame.Parent = sectionInners
                dropFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                dropFrame.BackgroundTransparency = 1.000
                dropFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
                dropFrame.BorderSizePixel = 0
                dropFrame.Position = UDim2.new(0, 0, 0.496453911, 0)
                dropFrame.Size = UDim2.new(0, 200, 0, 35)
                dropFrame.ClipsDescendants = true

                UIListLayout.Parent = dropFrame
                UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder

                dropOpenbtn.Name = "dropOpen"
                dropOpenbtn.Parent = dropFrame
                dropOpenbtn.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                dropOpenbtn.BackgroundTransparency = 0.500
                dropOpenbtn.BorderColor3 = Color3.fromRGB(0, 0, 0)
                dropOpenbtn.BorderSizePixel = 0
                dropOpenbtn.Size = UDim2.new(0, 200, 0, 35)
                dropOpenbtn.Font = Enum.Font.SourceSans
                dropOpenbtn.Text = ""
                dropOpenbtn.TextColor3 = Color3.fromRGB(0, 0, 0)
                dropOpenbtn.TextSize = 14.000
                dropOpenbtn.ClipsDescendants = true
                dropOpenbtn.MouseButton1Click:Connect(function()
                    if not focusing then
                        if opened then
                            opened = false
                            dropOpen.ImageTransparency = 1
                            dropClosed.ImageTransparency = 0
                            dropFrame:TweenSize(UDim2.new(0, 352, 0, 33), "InOut", "Linear", 0.08)
                            wait(0.1)
                            updateSectionFrame()
                            UpdateSize()
                        else
                            opened = true
                            dropOpen.ImageTransparency = 0
                            dropClosed.ImageTransparency = 1
                            dropFrame:TweenSize(UDim2.new(0, 352, 0, UIListLayout.AbsoluteContentSize.Y), "InOut", "Linear", 0.08, true)
                            wait(0.1)
                            updateSectionFrame()
                            UpdateSize()
                        end
                    end
                end)

                dropInfo.Name = "dropInfo"
                dropInfo.Parent = dropOpenbtn
                dropInfo.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                dropInfo.BackgroundTransparency = 1.000
                dropInfo.BorderColor3 = Color3.fromRGB(0, 0, 0)
                dropInfo.BorderSizePixel = 0
                dropInfo.Position = UDim2.new(0.171000004, 0, 0.119999997, 0)
                dropInfo.Size = UDim2.new(0, 165, 0, 24)
                dropInfo.Font = Enum.Font.Code
                dropInfo.Text = dropname
                dropInfo.TextColor3 = Color3.fromRGB(155, 155, 155)
                dropInfo.TextSize = 14.000
                dropInfo.TextXAlignment = Enum.TextXAlignment.Left

                coverup.Name = "coverup"
                coverup.Parent = dropOpenbtn
                coverup.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                coverup.BackgroundTransparency = 0.700
                coverup.BorderColor3 = Color3.fromRGB(0, 0, 0)
                coverup.BorderSizePixel = 0
                coverup.Position = UDim2.new(0, 0, 0.922469676, 0)
                coverup.Size = UDim2.new(0, 200, 0, 2)

                dropClosed.Name = "dropClosed"
                dropClosed.Parent = dropOpenbtn
                dropClosed.BackgroundTransparency = 1.000
                dropClosed.Position = UDim2.new(0.0199999996, 0, 0.142857149, 0)
                dropClosed.Size = UDim2.new(0, 24, 0, 24)
                dropClosed.Image = "rbxassetid://8445470826"
                dropClosed.ImageColor3 = Color3.fromRGB(155, 155, 155)
                dropClosed.ImageRectOffset = Vector2.new(404, 404)
                dropClosed.ImageRectSize = Vector2.new(96, 96)

                dropOpen.Name = "dropOpen"
                dropOpen.Parent = dropOpenbtn
                dropOpen.BackgroundTransparency = 1.000
                dropOpen.Position = UDim2.new(0.0199999996, 0, 0.142857149, 0)
                dropOpen.Size = UDim2.new(0, 24, 0, 24)
                dropOpen.Image = "rbxassetid://8445470826"
                dropOpen.ImageColor3 = Color3.fromRGB(155, 155, 155)
                dropOpen.ImageRectOffset = Vector2.new(704, 304)
                dropOpen.ImageRectSize = Vector2.new(96, 96)
                dropOpen.ImageTransparency = 1

                updateSectionFrame()
                UpdateSize()

                for i, v in next, list do
                    local optionSelect = Instance.new("TextButton")
                    local coverup = Instance.new("Frame")
                    local optionInfo = Instance.new("TextLabel")

                    optionSelect.Name = "optionSelect"
                    optionSelect.Parent = dropFrame
                    optionSelect.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                    optionSelect.BackgroundTransparency = 0.500
                    optionSelect.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    optionSelect.BorderSizePixel = 0
                    optionSelect.Size = UDim2.new(0, 200, 0, 35)
                    optionSelect.Visible = false
                    optionSelect.Font = Enum.Font.SourceSans
                    optionSelect.Text = "  "..v
                    optionSelect.TextColor3 = Color3.fromRGB(0, 0, 0)
                    optionSelect.TextSize = 14.000
                    optionSelect.ClipsDescendants = true
                    optionSelect.MouseButton1Click:Connect(function()
                        if not focusing then
                            opened = false
                            callback(v)
                            itemTextbox.Text = v
                            dropFrame:TweenSize(UDim2.new(0, 352, 0, 33), 'InOut', 'Linear', 0.08)
                            wait(0.1)
                            updateSectionFrame()
                            UpdateSize()       
                        end
                    end)


                    coverup.Name = "coverup"
                    coverup.Parent = optionSelect
                    coverup.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                    coverup.BackgroundTransparency = 0.700
                    coverup.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    coverup.BorderSizePixel = 0
                    coverup.Position = UDim2.new(0, 0, 0.922469676, 0)
                    coverup.Size = UDim2.new(0, 200, 0, 2)

                    optionInfo.Name = "optionInfo"
                    optionInfo.Parent = optionSelect
                    optionInfo.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    optionInfo.BackgroundTransparency = 1.000
                    optionInfo.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    optionInfo.BorderSizePixel = 0
                    optionInfo.Position = UDim2.new(0.171000004, 0, 0.119999997, 0)
                    optionInfo.Size = UDim2.new(0, 165, 0, 24)
                    optionInfo.Font = Enum.Font.Code
                    optionInfo.Text = "God-Mode"
                    optionInfo.TextColor3 = Color3.fromRGB(155, 155, 155)
                    optionInfo.TextSize = 14.000
                    optionInfo.TextXAlignment = Enum.TextXAlignment.Left
                end
            end

            function Elements:NewKeybind(keytext, first, callback)
                keytext = keytext or "KeybindText"
                callback = callback or function() end
                local oldKey = first.Name

                local keybindElement = Instance.new("TextButton")
                local bindName = Instance.new("TextLabel")
                local letter = Instance.new("ImageLabel")
                local coverup = Instance.new("Frame")
                local bind = Instance.new("TextLabel")

                keybindElement.Name = "keybindElement"
                keybindElement.Parent = sectionInners
                keybindElement.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                keybindElement.BackgroundTransparency = 0.500
                keybindElement.BorderColor3 = Color3.fromRGB(0, 0, 0)
                keybindElement.BorderSizePixel = 0
                keybindElement.Size = UDim2.new(0, 200, 0, 35)
                keybindElement.Font = Enum.Font.SourceSans
                keybindElement.Text = ""
                keybindElement.TextColor3 = Color3.fromRGB(0, 0, 0)
                keybindElement.TextSize = 14.000
                keybindElement.MouseButton1Click:connect(function(e) 
                    if not focusing then
                        bindName.Text = ". . ."
                        local a, b = game:GetService('UserInputService').InputBegan:wait();
                        if a.KeyCode.Name ~= "Unknown" then
                            bindName.Text = a.KeyCode.Name
                            oldKey = a.KeyCode.Name;
                        end
                    end
                end)

                UserInputService.InputBegan:connect(function(current, ok) 
                    if not ok then 
                        if current.KeyCode.Name == oldKey then 
                            callback()
                        end
                    end
                end)

                bindName.Name = "bindName"
                bindName.Parent = keybindElement
                bindName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                bindName.BackgroundTransparency = 1.000
                bindName.BorderColor3 = Color3.fromRGB(0, 0, 0)
                bindName.BorderSizePixel = 0
                bindName.Position = UDim2.new(0.171000361, 0, 0.120000347, 0)
                bindName.Size = UDim2.new(0, 90, 0, 24)
                bindName.Font = Enum.Font.Code
                bindName.Text = keytext
                bindName.TextColor3 = Color3.fromRGB(155, 155, 155)
                bindName.TextSize = 14.000
                bindName.TextXAlignment = Enum.TextXAlignment.Left

                letter.Name = "letter"
                letter.Parent = keybindElement
                letter.BackgroundTransparency = 1.000
                letter.Position = UDim2.new(0.0299999993, 0, 0.200142995, 0)
                letter.Size = UDim2.new(0, 20, 0, 20)
                letter.Image = "rbxassetid://8445470559"
                letter.ImageColor3 = Color3.fromRGB(155, 155, 155)
                letter.ImageRectOffset = Vector2.new(4, 404)
                letter.ImageRectSize = Vector2.new(96, 96)

                coverup.Name = "coverup"
                coverup.Parent = keybindElement
                coverup.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                coverup.BackgroundTransparency = 0.700
                coverup.BorderColor3 = Color3.fromRGB(0, 0, 0)
                coverup.BorderSizePixel = 0
                coverup.Position = UDim2.new(0, 0, 0.922469676, 0)
                coverup.Size = UDim2.new(0, 200, 0, 2)

                bind.Name = "bind"
                bind.Parent = keybindElement
                bind.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                bind.BackgroundTransparency = 1.000
                bind.BorderColor3 = Color3.fromRGB(0, 0, 0)
                bind.BorderSizePixel = 0
                bind.Position = UDim2.new(0.836000383, 0, 0.120000347, 0)
                bind.Size = UDim2.new(0, 30, 0, 24)
                bind.Font = Enum.Font.Code
                bind.Text = oldKey
                bind.TextColor3 = Color3.fromRGB(155, 155, 155)
                bind.TextSize = 14.000

                updateSectionFrame()
                UpdateSize()
            end
            return Elements
        end
        return Sections
    end
    return Tabs
end
return Prosperity
